{
  "canisters": {
    "zapierAutomationBackend": {
      "type": "custom",
      "wasm": "target/wasm32-unknown-unknown/release/zapier_automation_backend.wasm",
      "candid": "src/zapier-automation-backend/zapier-automation-backend.did",
      "build": [
        "cargo build --target wasm32-unknown-unknown --release",
        "sh -c 'candid-extractor target/wasm32-unknown-unknown/release/zapier_automation_backend.wasm > src/zapier-automation-backend/zapier-automation-backend.did'"
      ]
    }
  },
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "dfx": "0.18.0"
}
